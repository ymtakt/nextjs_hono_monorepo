{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "moduleResolution": "bundler",
    "target": "ES2017",
    "module": "esnext"
  },
  "exclude": ["node_modules"]
}
